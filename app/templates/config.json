{
    "source":        "source",
    "dest":          "dist",
    "tmp":           ".tmp",
    "file_dest":     "<% if (matrix) { %>mysource_<% } %>files",
    "module_prefix": "squiz-module-",
    "bower_ignore": [
        "jquery",
        "modernizr",
        "bootstrap-sass",
        "foundation",
        "datatables"
    ],
    "autoprefixer": true,
    "browsers": [
        "> 1%",
        "last 2 versions",
        "Firefox ESR",
        "Opera 12.1",
        <% if (ie8) { %>"IE 8",<% } %>
        "IE 9"
    ]
}