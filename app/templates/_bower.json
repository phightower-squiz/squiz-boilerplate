{
  "name": "<%= _.slugify(name) %>",
  "version": "<%= version %>",
  "private": true,
  "dependencies": {
    <% if (modules.length >= 1) {
        _.each(modules, function (module) {
    %>"<%= module.name %>": "<%= module.repository %>",
    <% })} %>
    <% if (includeBootstrap) { %>
    "bootstrap-sass-official": "*",
    <% } %>
    <% if (includeBourbon) { %>
    "bourbon": "3.x",
    <% } %>
    "modernizr": "~2.x",
    <% if (ie8) { %>
    "jquery": "~1.x",
    <% } else { %>
    "jquery": "*",
    <% } %>
    "qunit": "~1.x"
  }
}