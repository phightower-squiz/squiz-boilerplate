{
  "name": "<%= _.slugify(name) %>",
  "version": "<%= version %>",
  "dependencies": {
    <% if (modules.length >= 1) {
        _.each(modules, function (module) {
    %>"<%= module.name %>": "<%= module.repository %>",
    <% })} %>
    "modernizr": "~2.6.2",
    "jquery": "<%= jqueryVersion %>"
  }
}