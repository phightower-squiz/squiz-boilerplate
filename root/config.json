{
    "modules": [
        "accordion",
        "back_to_top",
        "breadcrumbs",
        "button",
        "collapsible_box",
        "header_logo",
        "overlay",
        "forms",
        "google_analytics",
        "skip_links",
        "responsive_image",
        "responsive_video",
        "tabs",
        "tables",
        "tables_sortable",
        "tooltip",
        "toggle_switch",
        "nav_basic",
        "nav_secondary",
        "nav_dropdown",
        "quick_search",
        "slideshow"
    ],
    "ie_fallback": {
        "IE7": "if lte IE 7",
        "IE8": "if lte IE 8",
        "IE9": "if lte IE 9"
    },
    "css": {
        "global": {
            "media":  "screen",
            "source": "global.scss",
            "output": "global.css"
        },
        "medium": {
            "ie_fallback": "IE8",
            "media": "(min-width: 37.5em)",
            "source": "medium.scss",
            "output": "medium.css",
            "comment": "600px or higher (with font size set @ 16px)"
        },
        "wide": {
            "ie_fallback": "IE8",
            "media": "(min-width: 60em)",
            "source": "wide.scss",
            "output": "wide.css",
            "comment": "960px or higher (with font size set @ 16px)"
        },
        "examples": {
            "parse_only": true,
            "source": "examples.scss",
            "output": "examples.css"
        },
        "print": {
            "media":  "print",
            "source": "print.scss",
            "output": "print.css"
        }
    },
    "source": {
        "core":    "source/core/",
        "libs":    "source/libs/",
        "modules": "source/modules/"
    },
    "destination": "dist",
    "temp_dir":    "tmp",
    "file_dest":   "mysource_files"
}